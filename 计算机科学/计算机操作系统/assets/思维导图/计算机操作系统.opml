        <outline text="特征" _mubu_text="%3Cspan%3E%E7%89%B9%E5%BE%81%3C/span%3E" _note="" _mubu_note="">
          <outline text="动态性" _mubu_text="%3Cspan%3E%E5%8A%A8%E6%80%81%E6%80%A7%3C/span%3E" _note="最基本特征。进程是程序的一次执行过程，是动态地产生、变化和消亡的​" _mubu_note="%3Cspan%3E%E6%9C%80%E5%9F%BA%E6%9C%AC%E7%89%B9%E5%BE%81%E3%80%82%E8%BF%9B%E7%A8%8B%E6%98%AF%E7%A8%8B%E5%BA%8F%E7%9A%84%E4%B8%80%E6%AC%A1%E6%89%A7%E8%A1%8C%E8%BF%87%E7%A8%8B%EF%BC%8C%E6%98%AF%E5%8A%A8%E6%80%81%E5%9C%B0%E4%BA%A7%E7%94%9F%E3%80%81%E5%8F%98%E5%8C%96%E5%92%8C%E6%B6%88%E4%BA%A1%E7%9A%84%3C/span%3E"/>
          <outline text="并发性" _mubu_text="%3Cspan%3E%E5%B9%B6%E5%8F%91%E6%80%A7%3C/span%3E" _note="内存中有多个进程实体，各进程可以并行执行" _mubu_note="%3Cspan%3E%E5%86%85%E5%AD%98%E4%B8%AD%E6%9C%89%E5%A4%9A%E4%B8%AA%E8%BF%9B%E7%A8%8B%E5%AE%9E%E4%BD%93%EF%BC%8C%E5%90%84%E8%BF%9B%E7%A8%8B%E5%8F%AF%E4%BB%A5%E5%B9%B6%E8%A1%8C%E6%89%A7%E8%A1%8C%3C/span%3E"/>
          <outline text="独立性" _mubu_text="%3Cspan%3E%E7%8B%AC%E7%AB%8B%E6%80%A7%3C/span%3E" _note="进程是能独立运行、获得资源、接受调度的基本单位" _mubu_note="%3Cspan%3E%E8%BF%9B%E7%A8%8B%E6%98%AF%E8%83%BD%E7%8B%AC%E7%AB%8B%E8%BF%90%E8%A1%8C%E3%80%81%E8%8E%B7%E5%BE%97%E8%B5%84%E6%BA%90%E3%80%81%E6%8E%A5%E5%8F%97%E8%B0%83%E5%BA%A6%E7%9A%84%E5%9F%BA%E6%9C%AC%E5%8D%95%E4%BD%8D%3C/span%3E"/>
          <outline text="异步性" _mubu_text="%3Cspan%3E%E5%BC%82%E6%AD%A5%E6%80%A7%3C/span%3E" _note="各进程按各自独立的、不可预测的速度向前推进，异步性会导致并发程序执行结果的不确定性，操作系统要提供“进程同步机制”来解决异步问题" _mubu_note="%3Cspan%3E%E5%90%84%E8%BF%9B%E7%A8%8B%E6%8C%89%E5%90%84%E8%87%AA%E7%8B%AC%E7%AB%8B%E7%9A%84%E3%80%81%E4%B8%8D%E5%8F%AF%E9%A2%84%E6%B5%8B%E7%9A%84%E9%80%9F%E5%BA%A6%E5%90%91%E5%89%8D%E6%8E%A8%E8%BF%9B%EF%BC%8C%E5%BC%82%E6%AD%A5%E6%80%A7%E4%BC%9A%E5%AF%BC%E8%87%B4%E5%B9%B6%E5%8F%91%E7%A8%8B%E5%BA%8F%E6%89%A7%E8%A1%8C%E7%BB%93%E6%9E%9C%E7%9A%84%E4%B8%8D%E7%A1%AE%E5%AE%9A%E6%80%A7%EF%BC%8C%E6%93%8D%E4%BD%9C%E7%B3%BB%E7%BB%9F%E8%A6%81%E6%8F%90%E4%BE%9B%E2%80%9C%E8%BF%9B%E7%A8%8B%E5%90%8C%E6%AD%A5%E6%9C%BA%E5%88%B6%E2%80%9D%E6%9D%A5%E8%A7%A3%E5%86%B3%E5%BC%82%E6%AD%A5%E9%97%AE%E9%A2%98%3C/span%3E"/>
          <outline text="结构性" _mubu_text="%3Cspan%3E%E7%BB%93%E6%9E%84%E6%80%A7%3C/span%3E" _note="每一个进程都会配置PCB" _mubu_note="%3Cspan%3E%E6%AF%8F%E4%B8%80%E4%B8%AA%E8%BF%9B%E7%A8%8B%E9%83%BD%E4%BC%9A%E9%85%8D%E7%BD%AEPCB%3C/span%3E"/>
        </outline>
        <outline text="程序顺序执行特性" _mubu_text="%3Cspan%3E%E7%A8%8B%E5%BA%8F%E9%A1%BA%E5%BA%8F%E6%89%A7%E8%A1%8C%E7%89%B9%E6%80%A7%3C/span%3E" _note="" _mubu_note="">
          <outline text="顺序性" _mubu_text="%3Cspan%3E%E9%A1%BA%E5%BA%8F%E6%80%A7%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="封闭性" _mubu_text="%3Cspan%3E%E5%B0%81%E9%97%AD%E6%80%A7%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="可再现性" _mubu_text="%3Cspan%3E%E5%8F%AF%E5%86%8D%E7%8E%B0%E6%80%A7%3C/span%3E" _note="" _mubu_note=""/>
        </outline>
        <outline text="程序并发执行特性" _mubu_text="%3Cspan%3E%E7%A8%8B%E5%BA%8F%E5%B9%B6%E5%8F%91%E6%89%A7%E8%A1%8C%E7%89%B9%E6%80%A7%3C/span%3E" _note="" _mubu_note="">
          <outline text="间断性" _mubu_text="%3Cspan%3E%E9%97%B4%E6%96%AD%E6%80%A7%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="失去封闭性" _mubu_text="%3Cspan%3E%E5%A4%B1%E5%8E%BB%E5%B0%81%E9%97%AD%E6%80%A7%3C/span%3E" _note="" _mubu_note=""/>
          <outline text="不可再现性" _mubu_text="%3Cspan%3E%E4%B8%8D%E5%8F%AF%E5%86%8D%E7%8E%B0%E6%80%A7%3C/span%3E" _note="" _mubu_note=""/>
        </outline>
      </outline>
      <outline text="进程的状态与转换" _mubu_text="%3Cspan%3E%E8%BF%9B%E7%A8%8B%E7%9A%84%E7%8A%B6%E6%80%81%E4%B8%8E%E8%BD%AC%E6%8D%A2%3C/span%3E" _note="" _mubu_note="">
        <outline text="进程的状态" _mubu_text="%3Cspan%3E%E8%BF%9B%E7%A8%8B%E7%9A%84%E7%8A%B6%E6%80%81%3C/span%3E" _note="基本状态：运行、就绪、阻塞/等待态" _mubu_note="%3Cspan%3E%E5%9F%BA%E6%9C%AC%E7%8A%B6%E6%80%81%EF%BC%9A%E8%BF%90%E8%A1%8C%E3%80%81%E5%B0%B1%E7%BB%AA%E3%80%81%E9%98%BB%E5%A1%9E/%E7%AD%89%E5%BE%85%E6%80%81%3C/span%3E">
          <outline text="运行状态(Running)" _mubu_text="%3Cspan%3E%E8%BF%90%E8%A1%8C%E7%8A%B6%E6%80%81(Running)%3C/span%3E" _note="占有CPU，并在CPU上运行
单核处理器一个时刻只有一个进程处于运行态，双核两个​" _mubu_note="%3Cspan%3E%E5%8D%A0%E6%9C%89CPU%EF%BC%8C%E5%B9%B6%E5%9C%A8CPU%E4%B8%8A%E8%BF%90%E8%A1%8C%0A%E5%8D%95%E6%A0%B8%E5%A4%84%E7%90%86%E5%99%A8%E4%B8%80%E4%B8%AA%E6%97%B6%E5%88%BB%E5%8F%AA%E6%9C%89%E4%B8%80%E4%B8%AA%E8%BF%9B%E7%A8%8B%E5%A4%84%E4%BA%8E%E8%BF%90%E8%A1%8C%E6%80%81%EF%BC%8C%E5%8F%8C%E6%A0%B8%E4%B8%A4%E4%B8%AA%3C/span%3E"/>
          <outline text="就绪状态(Ready)" _mubu_text="%3Cspan%3E%E5%B0%B1%E7%BB%AA%E7%8A%B6%E6%80%81(Ready)%3C/span%3E" _note="进程已经具备一切运行条件，除了没有空闲CPU，导致暂时不能运行​
万事俱备，只差CPU​" _mubu_note="%3Cspan%3E%E8%BF%9B%E7%A8%8B%E5%B7%B2%E7%BB%8F%E5%85%B7%E5%A4%87%E4%B8%80%E5%88%87%E8%BF%90%E8%A1%8C%E6%9D%A1%E4%BB%B6%EF%BC%8C%E9%99%A4%E4%BA%86%E6%B2%A1%E6%9C%89%E7%A9%BA%E9%97%B2CPU%EF%BC%8C%E5%AF%BC%E8%87%B4%E6%9A%82%E6%97%B6%E4%B8%8D%E8%83%BD%E8%BF%90%E8%A1%8C%0A%E4%B8%87%E4%BA%8B%E4%BF%B1%E5%A4%87%EF%BC%8C%E5%8F%AA%E5%B7%AECPU%3C/span%3E"/>
          <outline text="阻塞状态(Waiting/Blocked)" _mubu_text="%3Cspan%3E%E9%98%BB%E5%A1%9E%E7%8A%B6%E6%80%81(Waiting/Blocked)%3C/span%3E" _note="等待某一事件而暂时不能运行，比如等待操作系统分配打印机、等待磁盘读写
为了提高CPU利用效率，进程需要先准备好其它所需资源，才能得到CPU的服务​" _mubu_note="%3Cspan%3E%E7%AD%89%E5%BE%85%E6%9F%90%E4%B8%80%E4%BA%8B%E4%BB%B6%E8%80%8C%E6%9A%82%E6%97%B6%E4%B8%8D%E8%83%BD%E8%BF%90%E8%A1%8C%EF%BC%8C%E6%AF%94%E5%A6%82%E7%AD%89%E5%BE%85%E6%93%8D%E4%BD%9C%E7%B3%BB%E7%BB%9F%E5%88%86%E9%85%8D%E6%89%93%E5%8D%B0%E6%9C%BA%E3%80%81%E7%AD%89%E5%BE%85%E7%A3%81%E7%9B%98%E8%AF%BB%E5%86%99%0A%E4%B8%BA%E4%BA%86%E6%8F%90%E9%AB%98CPU%E5%88%A9%E7%94%A8%E6%95%88%E7%8E%87%EF%BC%8C%E8%BF%9B%E7%A8%8B%E9%9C%80%E8%A6%81%E5%85%88%E5%87%86%E5%A4%87%E5%A5%BD%E5%85%B6%E5%AE%83%E6%89%80%E9%9C%80%E8%B5%84%E6%BA%90%EF%BC%8C%E6%89%8D%E8%83%BD%E5%BE%97%E5%88%B0CPU%E7%9A%84%E6%9C%8D%E5%8A%A1%3C/span%3E"/>
          <outline text="创建状态(New)" _mubu_text="%3Cspan%3E%E5%88%9B%E5%BB%BA%E7%8A%B6%E6%80%81(New)%3C/span%3E" _note="操作系统为该进程分配所需内存等系统资源，为其创建、初始化PCB(分配PID等等)" _mubu_note="%3Cspan%3E%E6%93%8D%E4%BD%9C%E7%B3%BB%E7%BB%9F%E4%B8%BA%E8%AF%A5%E8%BF%9B%E7%A8%8B%E5%88%86%E9%85%8D%E6%89%80%E9%9C%80%E5%86%85%E5%AD%98%E7%AD%89%E7%B3%BB%E7%BB%9F%E8%B5%84%E6%BA%90%EF%BC%8C%E4%B8%BA%E5%85%B6%E5%88%9B%E5%BB%BA%E3%80%81%E5%88%9D%E5%A7%8B%E5%8C%96PCB(%E5%88%86%E9%85%8DPID%E7%AD%89%E7%AD%89)%3C/span%3E"/>
          <outline text="终止状态(Terminated)" _mubu_text="%3Cspan%3E%E7%BB%88%E6%AD%A2%E7%8A%B6%E6%80%81(Terminated)%3C/span%3E" _note="进程运行结束，或者出现Bug导致无法继续执行，操作系统需要撤销进程
完成资源回收，撤销PCB​​" _mubu_note="%3Cspan%3E%E8%BF%9B%E7%A8%8B%E8%BF%90%E8%A1%8C%E7%BB%93%E6%9D%9F%EF%BC%8C%E6%88%96%E8%80%85%E5%87%BA%E7%8E%B0Bug%E5%AF%BC%E8%87%B4%E6%97%A0%E6%B3%95%E7%BB%A7%E7%BB%AD%E6%89%A7%E8%A1%8C%EF%BC%8C%E6%93%8D%E4%BD%9C%E7%B3%BB%E7%BB%9F%E9%9C%80%E8%A6%81%E6%92%A4%E9%94%80%E8%BF%9B%E7%A8%8B%0A%E5%AE%8C%E6%88%90%E8%B5%84%E6%BA%90%E5%9B%9E%E6%94%B6%EF%BC%8C%E6%92%A4%E9%94%80PCB%E2%80%8B%3C/span%3E"/>
        </outline>
        <outline text="进程间的转换" _mubu_text="%3Cspan%3E%E8%BF%9B%E7%A8%8B%E9%97%B4%E7%9A%84%E8%BD%AC%E6%8D%A2%3C/span%3E" _note="" _mubu_note="">

            <outline text="根据进程状态不同，建立几张索引表" _mubu_text="%3Cspan%3E%E6%A0%B9%E6%8D%AE%E8%BF%9B%E7%A8%8B%E7%8A%B6%E6%80%81%E4%B8%8D%E5%90%8C%EF%BC%8C%E5%BB%BA%E7%AB%8B%E5%87%A0%E5%BC%A0%E7%B4%A2%E5%BC%95%E8%A1%A8%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="操作系统持有指向各索引表的指针" _mubu_text="%3Cspan%3E%E6%93%8D%E4%BD%9C%E7%B3%BB%E7%BB%9F%E6%8C%81%E6%9C%89%E6%8C%87%E5%90%91%E5%90%84%E7%B4%A2%E5%BC%95%E8%A1%A8%E7%9A%84%E6%8C%87%E9%92%88%3C/span%3E" _note="" _mubu_note=""/>
            <outline text="" _mubu_text="" _note="" _mubu_note="" _mubu_images="%5B%7B%22id%22%3A%221071735ad2c7c9142-6980721%22%2C%22oh%22%3A634%2C%22ow%22%3A1021%2C%22uri%22%3A%22document_image%2F7b84097b-4e0a-407a-841e-197d36e11848-6980721.jpg%22%2C%22w%22%3A458%7D%5D"/>
